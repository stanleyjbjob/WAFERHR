	</appSettings>
	<connectionStrings>
  <add name="syanghrConnectionString" connectionString="Data Source=jb-tony\sql2005;Initial Catalog=WAFERHR_ezpms;Persist Security Info=True;Password=JB8421;User ID=jb"
