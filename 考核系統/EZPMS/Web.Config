  <add name="TMTHRConnectionString" connectionString="Data Source=jb-tony\sql2005;Initial Catalog=WAFERHR_ezpms;Persist Security Info=True;User ID=jb;Password=JB8421"
   providerName="System.Data.SqlClient" />
  <add name="ezFlowConnectionString" connectionString="Data Source=jb-tony\sql2005;Initial Catalog=ezFlow_jb;Persist Security Info=True;User ID=jb;Password=JB8421"
