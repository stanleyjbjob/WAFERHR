            <MainSource>
              <DbSource ConnectionRef="syanghrConnectionString (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT         DEPT.D_NO, DEPT.D_NAME, DEPT.DEPT_GROUP, deptmang.Nobr, 
                          deptmang.name_c, deptmang.dept, deptmang.job_name, deptmang.jobl
FROM             DEPT LEFT OUTER JOIN
                              (SELECT         base.Nobr, base.name_c, basetts.dept, job.job_name, 
                                                           basetts.jobl
                                FROM              basetts INNER JOIN
                                                           base ON basetts.Nobr = base.Nobr INNER JOIN
                                                           job ON basetts.job = job.job
                                WHERE          (CONVERT(char(10), GETDATE(), 111) BETWEEN 
                                                           CONVERT(char(10), basetts.adate, 111) AND CONVERT(char(10), 
                                                           basetts.ddate, 111)) AND (basetts.ttscode IN ('1', '4', '6')) AND 
                                                           (basetts.mang = 1)) deptmang ON 
                          DEPT.D_NO = deptmang.dept
WHERE         (GETDATE() BETWEEN DEPT.ADATE AND DEPT.DDATE)
ORDER BY  DEPT.D_NO</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
